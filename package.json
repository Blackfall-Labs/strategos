{
  "name": "strategos",
  "version": "1.0.0",
  "description": "Your personal command center!",
  "main": "index.js",
  "scripts": {
    "build": "yarn clean && tsc -p ./tsconfig.json",
    "build:post": "yarn package",
    "clean": "rimraf ./dist",
    "prerelease": "yarn build",
    "pretest": "yarn clean && yarn build",
    "lint": "tslint -p tslint.json",
    "lint:fix": "yarn lint --fix",
    "package": "ts-node ./script/package.ts",
    "release": "git checkout master && git pull origin master && standard version && ts-node ./script/updatePackageVersion.ts",
    "ship": "git push --follow-tags origin master && cd ./dist && yarn publish",
    "": "",
    "test": "jest"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/TheGrimSilence/strategos.git"
  },
  "author": {
    "name": "Grim",
    "email": "silencegrim@gmail.com",
    "url": "https://thegrimsilence.com"
  },
  "license": "MIT",
  "keywords": [
    "command",
    "typescript",
    "command line",
    "typed",
    "class",
    "cli"
  ],
  "devDependencies": {
    "@types/node": "^12.12.11",
    "coveralls": "^3.0.8",
    "get-root-path": "^2.0.2",
    "jest": "^24.9.0",
    "jest-helpers": "^3.1.0",
    "rimraf": "^3.0.0",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "chalk": "^3.0.0"
  }
}
